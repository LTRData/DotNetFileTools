<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <Description>Show photo location on map</Description>
    <TargetFrameworks>net45;net6.0-windows</TargetFrameworks>
    <ApplicationIcon>icons8-country-80.ico</ApplicationIcon>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="LTRLib40">
      <HintPath>..\..\LTRLib\$(Configuration)\$(TargetFramework)\LTRLib40.dll</HintPath>
    </Reference>
    <Reference Include="LTRLib.Geodesy">
      <HintPath>..\..\LTRLib\$(Configuration)\$(TargetFramework)\LTRLib.Geodesy.dll</HintPath>
    </Reference>
    <Reference Include="LTRLib.PlaceMarks">
      <HintPath>..\..\LTRLib\$(Configuration)\$(TargetFramework)\LTRLib.PlaceMarks.dll</HintPath>
    </Reference>
    <Reference Include="LTRLib.Windows.Extended">
      <HintPath>..\..\LTRLib\$(Configuration)\$(TargetFramework)\LTRLib.Windows.Extended.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>